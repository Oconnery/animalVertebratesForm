<Window x:Class="Project_Darren.ViewFamily"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_Darren"
        mc:Ignorable="d"
        Title="ViewFamily" Height="300" Width="300">
    <Grid>
        <Label x:Name="label" Content="Family:" HorizontalAlignment="Left" Height="29" Margin="10,68,0,0" VerticalAlignment="Top" Width="72"/>
        <TextBox x:Name="vfamily_Input" HorizontalAlignment="Left" Height="29" Margin="87,68,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="189"/>
        <Button x:Name="OK_BT" Content="OK" HorizontalAlignment="Left" Height="24" Margin="95,140,0,0" VerticalAlignment="Top" Width="86" Click="OK_BT_Click"/>
        <Button x:Name="Exit_BT" Content="Cancel" HorizontalAlignment="Left" Height="24" Margin="186,140,0,0" VerticalAlignment="Top" Width="86" Click="Exit_BT_Click"/>

    </Grid>
</Window>
